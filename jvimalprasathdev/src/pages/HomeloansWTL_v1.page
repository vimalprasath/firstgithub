<script>
    $(function()
    {  
      $.getJSON("{!URLFOR($Resource.contactme,'/phoneNumbers.json')}", function(json, textStatus) {
                phoneNumbersArray = json; 
        var phoneNumber = findPhoneNumberBasedOnURLUsingJson();
        console.log(json);
        var $phoneNumberNode = $('.phoneNumber');
        
        $phoneNumberNode.empty();
        $phoneNumberNode.html(phoneNumber);
        if($('html').hasClass('touch')){
            $phoneNumberNode.attr({'href':"tel:+"+phoneNumber}); 
          $('.phoneNumber').click(function(){
            window.location= "tel:+"+phoneNumber;
          });

        }else{
          $phoneNumberNode.attr({'href':"javascript:e.preventDefault();"}); 
        }
        $.cookie('phoneNumber', phoneNumber);
        updateAlt(phoneNumber);
      });
      $("#source_url").val(window.location.href);
    });
  </script>
    

