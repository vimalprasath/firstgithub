    <validationRules>
        <fullName>EmpowerLeadID</fullName>
        <active>true</active>
        <description>Empower Lead ID cannot be NULL if Campaign Member Status is &quot;Application&quot;</description>
        <errorConditionFormula>AND( ISPICKVAL(Status,&quot;Application&quot;) , ISBLANK( Lead.Empower_Lead_ID__c  ) )</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>Cannot update campaign member status.  Navigate to lead detail page and enter an Empower Lead ID.</errorMessage>
