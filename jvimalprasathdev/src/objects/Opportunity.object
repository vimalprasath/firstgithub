    <webLinks>
        <fullName>Send_to_OPUS</fullName>
        <availability>online</availability>
        <description>Send the Opportunity to OPUS</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>OPUS</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>try
{
{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/apex.js&quot;)}

var empowerid=&quot;{!Opportunity.Empower_Lead_ID__c}&quot;;
if(!(empowerid) )
{

var userId = &quot;{!$User.Id}&quot;;
var ownerId = &quot;{!Opportunity.OwnerId}&quot;;

if(userId != ownerId){
alert(&quot;The owner of a Opportunity can only Send Opportunity to OPUS&quot;);
}

else{
var d = sforce.apex.execute(&quot;CreateOpusid_Opp&quot;,&quot;show&quot;,{OppId:&quot;{!Opportunity.Id}&quot;});
if (d == &apos;false&apos;){
alert(&quot;Please choose a Loan Purpose&quot;);
window.location.reload();}

else{
window.location.reload();}

}
}
else
{
var userId = &quot;{!$User.Id}&quot;;
var ownerId = &quot;{!Opportunity.OwnerId}&quot;;

if(userId != ownerId){
alert(&quot;The owner of a Opportunity can only View Opportunity in OPUS&quot;);
}

else{
// Changed by Richard for including FHA Lender ID - B-363560 
window.open(&quot;https://test/SolutionsFinder/index/welcome.vm?initialRouting=salesForceSolutionsFinder&amp;applicationView=RETAIL_CONSULTANT_CENTRALIZED&amp;lead={!Opportunity.Empower_Lead_ID__c}&amp;&amp;mloid={!$User.Id}&amp;operator={!$User.Alias}&amp;lenderid={!$User.Id}&quot;); 

}
}
}
catch (e)
{
alert(&quot;Due to network error, this opportunity has not been sent to OPUS&quot;);
window.location.reload();
}</url>
    </webLinks>
